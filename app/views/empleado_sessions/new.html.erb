
<h1>Login</h1>
<% form_tag empleado_sessions_path do%>
	<div class="field">
		<%= label_tag :username%>
		<%= text_field_tag :username%>
	</div>
	<div class="field">
		<%= label_tag :password%>
		<%= text_field_tag :password%>
	</div>
	<div class="actions">
		<%= submit_tag "login"%>
	</div>
<%end%>